    int add(){
        int a,b,c;
    printf("enter the value a&b");
    scanf("%d %d",&a,&b);
    c=a+b;
    printf("add & %d",c);
    }
    int sub(){
        int a,b,c;
    printf("enter the value a&b");
    scanf("%d%d",&a,&b);
    c=a-b;
    printf("add & %d",c);}
    int mul(){
        int a,b,c;
    printf("enter the value a&b");
    scanf("%d%d",&a,&b);
    c=a*b;
    printf("add & %d",c);}
    int div(){
        int a,b,c;
        printf("enter the value a&b");
    scanf("%d%d",&a,&b);
    c=a/b;
    printf("add & %d",c);
    }
    
    int main(){
         add();
    sub();
     mul();
     div();

